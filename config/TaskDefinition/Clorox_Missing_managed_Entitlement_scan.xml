  <Description>Creates missing managed entitlement objects for the applications specified.</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Parent>
    <Reference class="sailpoint.object.TaskDefinition" name="Missing Managed Entitlements Scan"/>
  </Parent>
</TaskDefinition>